package ear;
import java.util.Scanner;
public class Domain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		 String s=sc.next();
		dom(s);
	}
	public static void dom(String s) {
		String s1[] = s.split("\\");
		String s2 = s1[1];
		System.out.println(s2);
	}
}
